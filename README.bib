@book{horstemeyerIntegratedComputationalMaterials2012,
  title = {Integrated {{Computational Materials Engineering}} ({{ICME}}) for {{Metals}}: {{Using Multiscale Modeling}} to {{Invigorate Engineering Design}} with {{Science}}},
