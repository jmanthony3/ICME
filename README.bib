
@article{arsenlisDislocationDynamicsStudy2012,
  title = {A Dislocation Dynamics Study of the Transition from Homogeneous to Heterogeneous Deformation in Irradiated Body-Centered Cubic Iron},
  author = {Arsenlis, A. and Rhee, M. and Hommes, G. and Cook, R. and Marian, J.},
  year = {2012},
  month = may,
  journal = {Acta Materialia},
  volume = {60},
  number = {9},
  pages = {3748--3757},
  issn = {1359-6454},
  doi = {10.1016/j.actamat.2012.03.041},
  abstract = {Low temperature irradiation of crystalline materials is known to result in hardening and loss of ductility, which limits the usefulness of candidate materials in harsh nuclear environments. In body-centered cubic (bcc) metals, this mechanical property degradation is caused by the interaction of in-grown dislocations with irradiation defects, particularly small dislocation loops resulting from the microstructural evolution of displacement cascades. In this paper, we perform dislocation dynamics simulations of bcc Fe containing various concentrations of dislocation loops produced by irradiation in an attempt to gain insight into the processes that lead to hardening and embrittlement. We find that a transition from homogenous to highly localized deformation occurs at a critical loop density. Above it, plastic flow proceeds heterogeneously, creating defect-free channels in its wake. We find that channel initiation and size are mediated by loop coalescence resulting from elastic interactions with moving dislocations.},
  langid = {english},
  keywords = {Dislocation dynamics,Irradiated ferritic steels,Modeling and simulation,Plastic deformation},
  file = {C\:\\Users\\jmanthony1\\Zotero\\storage\\MCHR8WIK\\Arsenlis et al. - 2012 - A dislocation dynamics study of the transition fro.pdf;C\:\\Users\\jmanthony1\\Zotero\\storage\\DGYP289Q\\S1359645412002315.html}
}

